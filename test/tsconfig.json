{
    "compilerOptions": {
        "module": "commonjs",
        "noFallthroughCasesInSwitch": false,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "strictNullChecks": true,
        "target": "es6"
    },
    "exclude": [
        "end-to-end/rules/**/*",
        "../src/**/*.ts"
    ]
}
